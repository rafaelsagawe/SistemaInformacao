                <input asp-for="CNPJ" class="form-control" />
                <span asp-validation-for="CNPJ" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NProtocolo" class="control-label"></label>
                <input asp-for="NProtocolo" class="form-control" />
                <span asp-validation-for="NProtocolo" class="text-danger"></span>
            </div>
            <div class="form-group">

                <label asp-for="URG" class="control-label"></label>
                <select asp-for="URG" class="form-control" name="URG">
                    <option value="I">URG I</option>
                    <option value="II">URG II</option>
