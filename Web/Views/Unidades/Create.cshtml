                </select>
                <span asp-validation-for="URG" class="custom-select"></span>

                <!-- <label asp-for="URG" class="control-label"></label>
                  <input asp-for="URG" class="form-control" />
                  <span asp-validation-for="URG" class="text-danger"></span> -->
