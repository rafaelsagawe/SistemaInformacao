@model Web.Models.Unidades

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Unidades</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NomeUE" class="control-label"></label>
                <input asp-for="NomeUE" class="form-control" />
