@model IEnumerable<Web.Models.SupImp>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ModeloToner)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QtdImpressoas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QtdToner)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ModeloToner)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QtdImpressoas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QtdToner)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdSupImp">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdSupImp">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdSupImp">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
